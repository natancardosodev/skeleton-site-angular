{
    "name": "skeleton-angular-basic",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "test": "ng test",
        "stylelint": "./node_modules/.bin/stylelint 'src/**/**/*.scss'",
        "stylelint:fix": "./node_modules/.bin/stylelint 'src/**/**/*.scss' --fix",
        "eslint": "./node_modules/.bin/eslint \"src/app/**/*.ts\" --quiet",
        "eslint:fix": "./node_modules/.bin/eslint \"src/app/**/*.ts\" --fix",
        "lint": "npm run eslint && npm run stylelint",
        "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
        "check-version": "./node_modules/.bin/check-node-version --node 14 --npm 6",
        "clear": "rm -rf dist",
        "pre-build": "npm run check-version && npm run clear",
        "dev": "ng serve",
        "watch": "ng build --output-path dist --watch",
        "start": "npm run watch",
        "build": "npm run pre-build && npm run lint:fix && ng build --configuration=deve",
        "build-www": "npm run pre-build && npm run lint && ng build --configuration=production",
        "build-homo": "npm run pre-build && npm run lint && ng build --configuration=homologacao"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.0.0",
        "@angular/cdk": "^14.0.4",
        "@angular/common": "^14.0.0",
        "@angular/compiler": "^14.0.0",
        "@angular/core": "^14.0.0",
        "@angular/forms": "^14.0.0",
        "@angular/material": "^14.0.4",
        "@angular/platform-browser": "^14.0.0",
        "@angular/platform-browser-dynamic": "^14.0.0",
        "@angular/router": "^14.0.0",
        "bootstrap": "^5.1.3",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.0.4",
        "@angular/cli": "~14.0.4",
        "@angular/compiler-cli": "^14.0.0",
        "@types/jasmine": "~4.0.0",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/eslint-plugin-tslint": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "check-node-version": "^4.0.3",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jasmine-core": "~4.1.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "prettier": "^2.7.1",
        "prettier-eslint": "^15.0.1",
        "sass": "^1.45.2",
        "stylelint": "^14.9.1",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "typescript": "~4.7.2"
    }
}
